{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "worker": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-console": 0,
    "global-require": 0,
    "camelcase": 0,
    "linebreak-style": "off",
    "import/prefer-default-export": "off",
    "consistent-return": "off"
  }
}
